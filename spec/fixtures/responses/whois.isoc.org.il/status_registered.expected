  %s[2] CLASS(nameserver)
  %s[2].name == "aristo.tau.ac.il"
  %s[3] CLASS(nameserver)
  %s[3].name == "relay.huji.ac.il"
  %s[4] CLASS(nameserver)
  %s[4].name == "drns.isoc.org.il"
  %s[5] CLASS(nameserver)
