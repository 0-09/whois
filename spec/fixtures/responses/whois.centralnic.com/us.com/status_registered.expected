#referral_whois
  should: %s raise_error(Whois::PropertyNotSupported)

#referral_url
  should: %s raise_error(Whois::PropertyNotSupported)


#status
  should: %s == ["OK"]

#available?
  should: %s == false

#registered?
  should: %s == true


#created_on
  should: %s be_a(Time)
  should: %s == Time.parse("2003-10-20 10:03:28 UTC")

#updated_on
  should: %s be_a(Time)
  should: %s == Time.parse("2010-05-10 10:21:34 UTC")

#expires_on
  should: %s be_a(Time)
  should: %s == Time.parse("2014-10-20 23:59:59 UTC")


#nameservers
  should: %s be_a(Array)
  should: %s have(4).items
  should: %s[0] be_a(_nameserver)
  should: %s[0].name == "ns3.p17.dynect.net"
  should: %s[1] be_a(_nameserver)
  should: %s[1].name == "ns2.p17.dynect.net"
  should: %s[2] be_a(_nameserver)
  should: %s[2].name == "ns4.p17.dynect.net"
  should: %s[3] be_a(_nameserver)
  should: %s[3].name == "ns1.p17.dynect.net"
