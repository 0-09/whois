#status
  %s == :registered

#available?
  %s == false

#registered?
  %s == true


#created_on
  %s raise_error(Whois::AttributeNotSupported)

#updated_on
  %s CLASS(time)
  %s == Time.parse("2010-10-07")

#expires_on
  %s raise_error(Whois::AttributeNotSupported)


