#status
  %s == :registered

#available?
  %s == false

#registered?
  %s == true


#created_on
  %s CLASS(time)
  %s == Time.parse("2004-02-10 09:42:42")

#updated_on
  %s CLASS(time)
  %s == Time.parse("2010-04-19 21:47:14")

#expires_on
  %s CLASS(time)
  %s == Time.parse("2011-03-27 23:59:00")


#nameservers
  %s CLASS(array)
  %s SIZE(2)
  %s[0] CLASS(nameserver)
  %s[0].name == "ns1.google.com"
  %s[1] CLASS(nameserver)
  %s[1].name == "ns2.google.com"
